<!doctype html>
<html>
<head>
    <title>Rixx Music</title>
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
    <style>
        body         { padding-top:80px; word-wrap:break-word; }
    </style>
</head>
<body>
<div class="container">

    <div class="page-header text-center">
        <a href="/account" class="btn btn-default btn-sm">My Account</a>
        <a href="/settings" class="btn btn-default btn-sm">My Settings</a>
        <a href="/following" class="btn btn-default btn-sm">Following</a>
        <a href="/followers" class="btn btn-default btn-sm">Followers</a>
        <a href="/users" class="btn btn-default btn-sm">Users</a>
        <a href="/logout" class="btn btn-default btn-sm">Logout</a>
        

    </div>
    <% for(var i=0; i<result.length; i++) { %>
        <li> 
            <%= result[i] %>
            <a href='user?id=<%= result[i]._id %>' ><%= result[i].user_name %></a>
            <!-- <a href='follow?id=<%= result[i]._id %>&name=<%= result[i].name %>' class="btn btn-success btn-sm">Follow </a> -->
        </li>
    <% } %>
        
</div>
</body>
</html>
